{
  "name": "colorful",
  "displayName": "Colorful",
  "backgroundImage": "background.jpg",
  "inherits": "light",
  "mostLike": "light",
  "schemaVersion": 1,
  "colors": {
    "background": "transparent",
    "secondary": "hsl(210 100% 85%)",
    "secondaryForeground": "hsl(210 50% 25%)",
    "accent": "hsl(280 100% 85%)",
    "accentForeground": "hsl(280 50% 25%)",
    "card": "transparent",
    "cardBackdropFilter": "blur(28px) saturate(180%) brightness(1.15)",
    "popover": "transparent",
    "popoverBackdropFilter": "blur(24px) saturate(170%) brightness(1.15)"
  },
  "fonts": {
    "mono": "JetBrains Mono, Consolas, Monaco, monospace"
  },
  "buttons": {
    "outline": {
      "background": "transparent",
      "color": "hsl(0 0% 9%)",
      "border": "1px solid hsl(280 100% 85%)",
      "borderRadius": "0.75rem",
      "boxShadow": "none",
      "hover": {
        "background": "hsl(280 100% 85% / 0.1)",
        "color": "hsl(280 50% 25%)",
        "transform": "scale(1.02)"
      }
    },
    "secondary": {
      "background": "hsl(210 100% 85%)",
      "color": "hsl(210 50% 25%)",
      "border": "1px solid hsl(210 100% 85%)",
      "borderRadius": "0.75rem",
      "boxShadow": "0 2px 8px hsl(210 100% 85% / 0.3)",
      "hover": {
        "background": "hsl(210 100% 80%)",
        "transform": "translateY(-1px)"
      }
    }
  },
  "tables": {
    "background": "rgba(255, 255, 255, 0.12)",
    "header": {
      "background": "rgba(10,132,255,0.12)"
    }
  },
  "buttonStyle": "shimmer",
  "switches": {
    "checked": {
      "background": "hsl(280 30% 60%)"
    },
    "unchecked": {
      "background": "hsl(280 90% 85%)"
    },
    "thumb": {
      "background": "hsl(0 0% 100%)"
    }
  }
}
